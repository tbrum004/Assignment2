    def modifyAge(self, str):
        ans = ""
        for i,val in enumerate(str):
            if((i+1)%3==0):
                ans += val.upper()
            elif((i+1)%4==0):
                ans += val.lower()
            elif((i+1)%5==0):
                ans += " "
            else:
                ans +=val

        return ans
